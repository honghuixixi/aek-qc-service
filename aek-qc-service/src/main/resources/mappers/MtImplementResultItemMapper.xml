<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.qc.mapper.MtImplementResultItemMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.qc.model.MtImplementResultItem">
		<id column="id" property="id" />
		<result column="result_id" property="resultId" />
		<result column="plan_template_id" property="planTemplateId" />
		<result column="plan_template_item_id" property="planTemplateItemId" />
		<result column="plan_template_item_name" property="planTemplateItemName" />
		<result column="item_result" property="itemResult" />
		<result column="item_remarks" property="itemRemarks" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, result_id AS resultId, plan_template_id AS planTemplateId, plan_template_item_id AS planTemplateItemId, plan_template_item_name AS planTemplateItemName, item_result AS itemResult, item_remarks AS itemRemarks
    </sql>
</mapper>
