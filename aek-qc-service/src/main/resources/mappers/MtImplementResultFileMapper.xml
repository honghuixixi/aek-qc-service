<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.qc.mapper.MtImplementResultFileMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.qc.model.MtImplementResultFile">
		<id column="id" property="id" />
		<result column="result_id" property="resultId" />
		<result column="file_name" property="fileName" />
		<result column="file_path" property="filePath" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, result_id AS resultId, file_name AS fileName, file_path AS filePath
    </sql>
</mapper>
